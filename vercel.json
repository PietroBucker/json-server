{
  "functions": {
    "api/server.js": {
      "memory": 1024,
      "includeFiles": "db.json"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "api/server.js"
    }
  ],
  "routes": [
    { "src": "/api/videos/front", "dest": "/videos/front" },
    { "src": "/api/videos/back", "dest": "/videos/back" },
    { "src": "/api/*", "dest": "/$1" }
  ]
}
